

generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
   url      = "postgresql://postgres:postgres@localhost:5432/reclamos"
}

model User {
  id       Int    @id @default(autoincrement())
  email    String @unique
  password String
  role     String
}
